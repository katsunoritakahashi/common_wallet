        <td class="text-right fs-12 border-top border-dark-top bottom">
          <% if @spending_food > @budget.food %>
            <span class="red"><%= "#{(@spending_food - @budget.food).to_s(:delimited)}円オーバー " %></span><i class="fas fa-fire fa-lg red fs-15"></i>
          <% else %> 
            <span><%= "あと#{((@spending_food - @budget.food)* -1).to_s(:delimited)}円" %></span>
          <% end %>
        </td>
      </tr>
      <tr>
        <td colspan="2">
        <div class="progress">
          <% if @budget.food != 0 %>
            <div class="progress-bar bg-warning" role="progressbar" style="<%= "width: #{@spending_food * 100 / @budget.food}%" %>" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"><%= "予算オーバー" if @spending_food > @budget.food %></div>
          <% else %>
             <div class="progress-bar bg-warning" role="progressbar" style="width: 0%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
          <% end %>
        </div>
        </td> 
      </tr>
      <tr>
        <td class="text-left top"><%= "支出#{@spending_food.to_s(:delimited)}円" %></td>
        <td class="text-right top"><%= "#{@budget.food.to_s(:delimited)}円" %></td>
      </tr>

      <tr>
        <td class="text-left fs-15 border-top border-dark-top bottom"><i class="fas fa-glass-cheers fa-lg green"></i> 交際費</td>
        <td class="text-right fs-12 border-top border-dark-top bottom">
